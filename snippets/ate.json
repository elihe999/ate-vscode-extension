{
    "forLoop": {
        "prefix": "foreach",
        "body": [
          "Foreach ${1:item} as ${2:array}",
          "\t${3:code}",
          "End"
        ],
        "description": "forLoop"
    },
    "vectorList": {
        "prefix": "Vector",
        "body": [
            "Vector ${1:vector name} = {${2:item}}"
        ]
    },
    "ifLoop$equal": {
        "prefix": "ifequal",
        "body": [
          "If ${1:parameter} equal \"${2:string}\":",
          "\t${3:code}",
          "End"
        ],
        "description": "forLoop"
    },
    "Region$name": {
        "prefix": "Region",
        "body": [
          "Region ${1:parameter}",
          "\t${1:name} = ${2:width} ${3:height} ${4:x} ${5:y}",
          "End"
        ],
        "description": "Screen capture Region"
    },
    "include": {
        "prefix": "Include",
        "body": [
            "Include \"${1:script}\""
        ]
    },
    "ifLoop$unequal": {
        "prefix": "ifun",
        "body": [
          "If ${1:parameter} unequal \"${2:string}\":",
          "\t${3:code}",
          "End"
        ],
        "description": "forLoop"
    },
    "errorLoop": {
        "prefix": "Error",
        "body": [
          "Error",
          "\tVar picName = \"Error picture\"",
          "\tForeach ${1:item} as ${2:array} ",
          "\t\t${3:code}",
          "\tEnd",
          "End"
        ],
        "description": "errorLoop"
    },
    "syslogPre$AUD": {
        "prefix": "syslog:AUD",
        "body": [
            "Syslog:AUD ${1:target} \"${2:string}\""
        ]
    },
    "syslogPre$CAP": {
        "prefix": "syslog:CAP",
        "body": [
            "Syslog:CAP ${1:target} \"${2:string}\""
        ]
    },
    "syslogPre$LED": {
        "prefix": "syslog:LED",
        "body": [
            "Syslog:LED ${1:target} \"${2:string}\""
        ]
    },
    "syslogPre$SCR": {
        "prefix": "syslog:SCR",
        "body": [
            "Syslog:SCR ${1:target} \"${2:string}\""
        ]
    },
    "syslogPre$STA": {
        "prefix": "syslog:STA",
        "body": [
            "Syslog:STA ${1:target} \"${2:string}\""
        ]
    },
    "pause$time": {
        "prefix": "Pause",
        "body": [
            "Pause ${1:timeMicrosecond}"
        ]
    },
    "init$device": {
        "prefix": "API:Init",
        "body": [
            "API:Init ${1:device}"
        ]
    },
    "keys$device": {
        "prefix": "API:Key",
        "body": [
            "API:Key ${1:device} \"${2:action}\" <${3:note}>"
        ]
    },
    "softkey$device": {
        "prefix": "API:SoftKey",
        "body": [
            "API:SoftKey ${1:device} \"${2:action}\" <${3:note}>"
        ]
    },
    "reset$device": {
        "prefix": "API:Reset",
        "body": [
            "API:Reset <${1:device}>"
        ]
    },
    "deploy$device": {
        "prefix": "Deploy",
        "body": [
            "Deploy ${1:device} \"${2:filename}\""
        ]
    },
    "string$Transform": {
        "prefix": "String:Transform",
        "body": [
            "String:Transform ${1:string} ${2:number}"
        ]
    },
    "string$Append": {
        "prefix": "String:Append",
        "body": [
            "String:Append ${1:string} ${2:number}"
        ]
    },
    "string$Substr": {
        "prefix": "String:Substr",
        "body": [
            "String:Substr ${1:string} ${2:begin} ${3:end}"
        ]
    },
    "string$DialStr": {
        "prefix": "String:DialStr",
        "body": [
            "String:DialStr ${1:string} ${2:number}"
        ]
    },
    "string$Replace": {
        "prefix": "String:Replace",
        "body": [
            "String:Replace ${1:string} ${2:number}"
        ]
    },
    "number$Add": {
        "prefix": "Number:Add",
        "body": [
            "Number:Add ${1:param} ${2:number}"
        ]
    },
    "number:$Transform": {
        "prefix": "Number:Transform",
        "body": [
            "Number:Transform ${1:newparam} ${2:string}"
        ]
    },
    "reboot$Device": {
        "prefix": "API:Reboot",
        "body": [
            "API:Reboot ${1:device}"
        ]
    },
    "config$TFTP": {
        "prefix": "API:Config",
        "body": [
            "API:Reboot ${1:device} serverURL",
            "API:Reboot ${1:device} ${2:CustomURL"
        ]
    },
    "capture:$Device": {
        "prefix": "API:Capture",
        "body": [
            "API:Capture ${1:device} ${2:pictureName}"
        ]
    },
    "Request:$Device": {
        "prefix": "Request:",
        "body": [
            "Request:${1:param} ${2:device} ${3:Pvalue}"
        ]
    },
    "Run:$Script": {
        "prefix": "Run ",
        "body": [
            "Run \"{1:filename}\" ${2:param}"
        ]
    },
    "Console:$Stop": {
        "prefix": "Console:Stop",
        "body": [
            "Console:Stop ThisCase",
            "Console:Stop ThisLoop"
        ]
    },
    "Console:$Log": {
        "prefix": "Console:Log",
        "body": [
            "Console:Log Normal ${1:param}",
            "Console:Log Warning ${1:param}",
            "Console:Log Error ${1:param}"
        ]
    },
    "arp:$scan": {
        "prefix": "Console:Arp",
        "body": [
            "Console:ArpScan\n"
        ]
    }
}
