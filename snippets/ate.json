{
    "forLoop": {
        "prefix": "foreach",
        "body": [
          "Foreach ${1:item} as ${2:array} ",
          "\t${3:code}",
          "End"
        ],
        "description": "forLoop"
    },
    "vectorList": {
        "prefix": "Vector",
        "body": [
            "Vector ${1:vector name} = {${2:item}}"
        ]
    },
    "ifLoop$equal": {
        "prefix": "ifequal",
        "body": [
          "If ${1:parameter} equal \"${2:string}\":",
          "\t${3:code}",
          "End"
        ],
        "description": "forLoop"
    },
    "ifLoop$unequal": {
        "prefix": "ifun",
        "body": [
          "If ${1:parameter} unequal \"${2:string}\":",
          "\t${3:code}",
          "End"
        ],
        "description": "forLoop"
    },
    "errorLoop": {
        "prefix": "Error",
        "body": [
          "Error",
          "\tVar picName = \"Error picture\"",
          "\tForeach ${1:item} as ${2:array} ",
          "\t\t${3:code}",
          "\tEnd",
          "End"
        ],
        "description": "errorLoop"
    },
    "syslogPre$AUD": {
        "prefix": "syslog:AUD",
        "body": [
            "Syslog:AUD ${1:target} \"${2:string}\""
        ]
    },
    "syslogPre$CAP": {
        "prefix": "syslog:CAP",
        "body": [
            "Syslog:CAP ${1:target} \"${2:string}\""
        ]
    },
    "syslogPre$LED": {
        "prefix": "syslog:LED",
        "body": [
            "Syslog:LED ${1:target} \"${2:string}\""
        ]
    },
    "syslogPre$SCR": {
        "prefix": "syslog:SCR",
        "body": [
            "Syslog:SCR ${1:target} \"${2:string}\""
        ]
    },
    "syslogPre$STA": {
        "prefix": "syslog:STA",
        "body": [
            "Syslog:STA ${1:target} \"${2:string}\""
        ]
    },
    "pause$time": {
        "prefix": "Pause",
        "body": [
            "Pause ${1:timeMicrosecond}"
        ]
    },
    "init$device": {
        "prefix": "API:Init",
        "body": [
            "API:Init ${1:device}"
        ]
    },
    "keys$device": {
        "prefix": "API:Key",
        "body": [
            "API:Key ${1:device} \"${2:action}\" <${3:note}>"
        ]
    },
    "softkey$device": {
        "prefix": "API:SoftKey",
        "body": [
            "API:SoftKey ${1:device} \"${2:action}\" <${3:note}>"
        ]
    },
    "reset$device": {
        "prefix": "API:Reset",
        "body": [
            "API:Reset <${1:device}>"
        ]
    },
    "deploy$device": {
        "prefix": "Deploy",
        "body": [
            "Deploy ${1:device} \"${2:filename}\""
        ]
    },
    "string$Transform": {
        "prefix": "String:T",
        "body": [
            "String:Transform ${1:string} ${2:number}"
        ]
    },
    "string$Append": {
        "prefix": "String:A",
        "body": [
            "String:Append ${1:string} ${2:number}"
        ]
    },
    "string$DialStr": {
        "prefix": "String:D",
        "body": [
            "String:DialStr ${1:string} ${2:number}"
        ]
    },
    "string$Replace": {
        "prefix": "String:R",
        "body": [
            "String:Replace ${1:string} ${2:number}"
        ]
    }
}
