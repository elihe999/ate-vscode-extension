{
  "scopeName": "source.gsate",
  "patterns": [{ "include": "#expression" }],
  "repository": {
    "expression": {
      "patterns": [{ "include": "#letter" }, { "include": "#paren-expression" }]
    },
    "letter": {
      "match": "(API|Syslog|Deploy|Run|Request)",
      "name": "keyword.letter",
      "include": "#letter-param"
    },
    "letter-param": {
      "match": "aaa",
      "name": "variable.other.bracket.ate"
    },
    "paren-expression": {
      "begin": "\\(",
      "end": "\\)",
      "beginCaptures": {
        "0": { "name": "punctuation.paren.open" }
      },
      "endCaptures": {
        "0": { "name": "punctuation.paren.close" }
      },
      "name": "expression.group",
      "patterns": [{ "include": "#expression" }]
    }
  }
}